<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="sonata.admin.dnna.model_manager" class="Psdtg\AdminBundle\Model\DnnaModelManager">
            <argument type="service" id="doctrine" />
        </service>
        <service id="sonata.admin.dnna.datagridbuilder" class="Psdtg\AdminBundle\Builder\DnnaDatagridBuilder">
            <argument type="service" id="form.factory" />
            <argument type="service" id="sonata.admin.builder.filter.factory" />
            <argument type="service" id="sonata.admin.guesser.orm_datagrid_chain" />
        </service>
        <service id="sonata.admin.users" class="Psdtg\AdminBundle\Admin\UserAdmin">
           <tag name="sonata.admin"
                manager_type="orm"
                group="Users"
                label="Users"
                model_manager="sonata.admin.dnna.model_manager"
                datagrid_builder="sonata.admin.dnna.datagridbuilder"
                />
           <argument />
           <argument>Psdtg\UserBundle\Entity\User</argument>
           <argument>SonataAdminBundle:CRUD</argument>
           <call method="setTranslationDomain">
               <argument>PsdtgAdminBundle</argument>
           </call>
           <call method="setUserManager">
               <argument type='service' id='fos_user.user_manager' />
           </call>
       </service>
   </services>
</container>