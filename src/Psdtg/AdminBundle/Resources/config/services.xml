<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="sonata.admin.dnna.model_manager" class="Psdtg\AdminBundle\Model\DnnaModelManager">
            <argument type="service" id="doctrine" />
        </service>
        <service id="sonata.admin.dnna.datagridbuilder" class="Psdtg\AdminBundle\Builder\DnnaDatagridBuilder">
            <argument type="service" id="form.factory" />
            <argument type="service" id="sonata.admin.builder.filter.factory" />
            <argument type="service" id="sonata.admin.guesser.orm_datagrid_chain" />
        </service>
        <service id="sonata.admin.telephonelines" class="Psdtg\AdminBundle\Admin\TelephoneLineAdmin">
           <tag name="sonata.admin"
                manager_type="orm"
                group="Telephone Lines"
                label="Telephone Lines"
                />
           <argument />
           <argument>Psdtg\SiteBundle\Entity\TelephoneLine</argument>
           <argument>SonataAdminBundle:CRUD</argument>
       </service>
        <service id="sonata.admin.newlinerequests" class="Psdtg\AdminBundle\Admin\NewLineRequestAdmin">
           <tag name="sonata.admin"
                manager_type="orm"
                group="Requests"
                label="New Line Request"
                />
           <argument />
           <argument>Psdtg\SiteBundle\Entity\Requests\NewLineRequest</argument>
           <argument>SonataAdminBundle:CRUD</argument>
           <call method="setSecurityContext">
               <argument type='service' id='security.context' />
           </call>
       </service>
   </services>
</container>